{
  "name": "faesign",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "visual:check": "node tests/visual/backstop/scripts/check-services.cjs",
    "visual:setup": "concurrently \"npm run dev\" \"npm run storybook\" --names \"Frontend,Storybook\" --prefix-colors \"blue,green\"",
    "visual:stop": "taskkill /f /im node.exe 2>nul || true",
    "visual:reference": "backstop reference --config=backstop-complete.config.cjs",
    "visual:test": "backstop test --config=backstop-complete.config.cjs",
    "visual:approve": "backstop approve --config=backstop-complete.config.cjs",
    "visual:staging": "backstop test --config=backstop-staging.config.cjs",
    "visual:staging:reference": "backstop reference --config=backstop-staging.config.cjs",
    "visual:minimal": "backstop test --config=backstop-minimal.config.cjs",
    "test:percy:local": "percy exec -- playwright test tests/visual/percy/",
    "test:percy:components": "percy exec -- playwright test tests/visual/percy/percy-components.spec.js",
    "test:percy:responsive": "percy exec -- playwright test tests/visual/percy/percy-responsive.spec.js",
    "test:percy:core": "percy exec -- playwright test tests/visual/percy/percy-core-pages.spec.js",
    "test:percy:dry-run": "percy exec --dry-run -- playwright test tests/visual/percy/",
    "test:playwright": "playwright test --trace on",
    "playwright:install": "playwright install"
  },
  "dependencies": {
    "bootstrap": "^5.3.7",
    "react": "^19.1.0",
    "react-bootstrap": "^2.10.10",
    "react-dom": "^19.1.0",
    "react-router-dom": "^7.7.1",
    "sweetalert2": "^11.22.2"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^3.2.7",
    "@eslint/js": "^9.25.0",
    "@percy/cli": "^1.31.1",
    "@percy/playwright": "^1.0.9",
    "@percy/storybook": "^9.0.0",
    "@playwright/test": "^1.54.2",
    "@storybook/addon-essentials": "^8.6.14",
    "@storybook/addon-interactions": "^8.6.14",
    "@storybook/react-vite": "^8.6.14",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@vitejs/plugin-react": "^4.4.1",
    "backstopjs": "^6.3.23",
    "concurrently": "^9.2.0",
    "eslint": "^9.25.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.0.0",
    "playwright": "^1.54.2",
    "storybook": "^8.6.14",
    "vite": "^6.3.5"
  },
  "eslintConfig": {
    "extends": [
      "plugin:storybook/recommended"
    ]
  }
}
